{
 "name": "Navi",
 "description": "Navigation menu",
 "version": "1.0",
 "manifest_version": 3,
 "permissions": [
  "webNavigation",
  "tabs",
  "history",
  "activeTab",
	 "storage",
	 "http://*/*",
  "https://*/*"
 ],
 "background": {
  "service_worker": "background.js"
 },
 "content_scripts": [{
  "matches": ["<all_urls>"],
  "js": ["script.js"]
 }],
 "web_accessible_resources": [{
  "matches": ["<all_urls>"],
  "resources": ["navi.js"]
 }]
}
